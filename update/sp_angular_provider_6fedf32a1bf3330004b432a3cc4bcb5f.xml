<?xml version="1.0" encoding="UTF-8"?><record_update table="sp_angular_provider">
    <sp_angular_provider action="INSERT_OR_UPDATE">
        <name>mmpMultiTask</name>
        <script><![CDATA[function mmpMultiTask()  {  
    var directive = {};
    directive.restrict = 'E';
    directive.scope = {
        title: '@',
        multiTaskDetails: '='
    }; 
    directive.template= '<div class="task-list" >'+
'    <h3 class="task-heading">{{title}}</h3>'+
'    <div class="task-list-wrapper">'+
'        <div class="task-list-single" ng-repeat="task in multiTaskDetails.tasks">'+
'            <div class="task-card">'+
'                <h3>{{task.name}}</h3>'+
'                <table>'+
'                    <tbody><tr>'+
'                        <td><i class="{{task.icon}}" aria-hidden="true"></i></td>'+
'                        <td><div class="small-progress-bar">'+
'                                <div class="current-progress" style="width: {{task.PercentComplete}}%;" ng-show="task.PercentComplete <= 99"></div>'+
'                                <div class="complete-progress" style="width: {{task.PercentComplete}}%;" ng-show="task.PercentComplete > 99"></div>'+
'                            </div>'+
'                        </td>'+
'                    </tr>'+
'                </tbody></table>'+
'                <div class="task-checkboxes">'+
''+
'                    <p ng-repeat="taskItems in task.listOfTasks" >'+
''+
'                        <a class="custom-check completeCheck" ng-if="taskItems.Complete">'+
'                            <i class="fa fa-check-circle-o" aria-hidden="true"></i>'+
'                            {{taskItems.name}}'+
'                        </a>'+
''+
'                        <a href="{{taskItems.taskUrl}}" class="custom-check" ng-hide="taskItems.Complete">'+
'                            <i class="{{taskItems.iconClass}}" aria-hidden="true" style="color:{{taskItems.iconClassColor}}"></i> '+
'                            <i class="fa fa-circle-o" aria-hidden="true" ng-hide="taskItems.iconClass"></i>'+
'                            {{taskItems.name}}'+
'                        </a>'+
'                    </p>'+
'                    '+
'                </div>'+
'                <div class="contact-task">'+
'                    <a href="{{task.contact.url}}" ng-hide="task.contact.url == \'\' || task.contact.name == \'\'">'+
'                        <i class="fa fa-phone-square" aria-hidden="true"></i> {{task.contact.name}}'+
'                    </a>'+
'                </div>'+
'                '+
'            </div>'+
'        </div>'+
'    </div>'+
'</div>';
    return directive;
}]]></script>
